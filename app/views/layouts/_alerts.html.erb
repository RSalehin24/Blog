<div id="alerts42">
  <div id="hideMe">
    <% if notice || alert %>
      <div class="alert alert-warning alert-dismissible fade show" role="alert" style="margin-bottom: 0px; margin-top: 0px;">
        <%= notice %>
        <%= alert %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    <% end %>
  </div>
</div> 
